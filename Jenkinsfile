pipeline{
    agent any
    
    stages{
        stage('init'){
            steps{
                script{
                    echo "Initialization..."
                }
            }
        }

        stage('test'){
            steps{
                script{
                    echo "Testing..."
                }
            }
        }

        stage('build'){
            steps{
                script{
                    echo "Building..."
                }
            }
        }

        stage('deploy'){
            steps{
                script{
                    echo "deploying..."
                }
            }
        }
    }
}